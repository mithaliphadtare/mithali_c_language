#include<stdio.h>
int main(){
int arr[4][5];
int i,j;
printf("enter the elements:");
for(i=0;i<4;i++){
for(int j=0;j<5;j++){
scanf("%d",&arr[i][j]);
}
}
printf("original:\n");
for(i=0;i<4;i++){
for(j=0;j<5;j++){
printf(" %d",arr[i][j]);
}
printf("\n");
}
printf("transpose:\n");
for(int j=0;j<5;j++){
for(i=0;i<4;i++){
printf(" %d",arr[i][j]);
}
printf("\n");
}
return 0;
}
