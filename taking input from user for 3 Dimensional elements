#include<stdio.h>
int main(){
int arr[2][2][2];
printf("enter elements for array:");
int i,j,k;
for(i=0;i<2;i++){
for(j=0;j<2;j++){
for(k=0;k<2;k++){
scanf("%d",&arr[i][j][k]);
}
}
}
for(i=0;i<2;i++){
for(j=0;j<2;j++){
for(k=0;k<2;k++){
printf(" %d",arr[i][j][k]);
}
printf("\t");
}
printf("\n");
}
return 0;
}
